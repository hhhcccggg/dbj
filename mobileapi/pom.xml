            <artifactId>token-center</artifactId>
            <version>1.0-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>com.zwdbj.server</groupId>
            <artifactId>common</artifactId>
            <version>1.0-SNAPSHOT</version>
